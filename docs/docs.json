{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Container Use",
  "description": "Sandboxed dev environments for agents to work safely in parallel. Powered by Dagger.",
  "colors": {
    "primary": "#0D9373",
    "light": "#07C983",
    "dark": "#0D9373"
  },
  "favicon": "/images/dagger-icon.png",
  "redirects": [
    {
      "source": "/discord",
      "destination": "https://discord.gg/NQDJTBC6Qk"
    }
  ],
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "introduction",
              "installation",
              "quickstart"
            ]
        },
        {
          "group": "Guides",
          "pages": [
            "environment-workflow",
            "environment-configuration",
            "secrets"
          ]
        },
          {
            "group": "Integrations",
            "pages": [
              "agent-integrations"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/dagger/container-use",
          "icon": "github"
        },
        {
          "anchor": "Discord",
          "href": "https://container-use.com/discord",
          "icon": "discord"
        }
      ]
    }
  },
  "logo": {
    "light": "/images/container-use.png",
    "dark": "/images/container-use.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
		"href": "https://container-use.com/discord"
      }
    ],
    "primary": {
      "type": "button",
      "label": "GitHub",
      "href": "https://github.com/dagger/container-use"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/dagger/container-use",
      "discord": "https://container-use.com/discord"
    }
  }
}
